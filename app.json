{
  "expo": {
    "name": "Dark Clock",
    "description": "Darkest Night Clock.",
    "slug": "darkest-night-clock",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": ["ios", "android"],
    "assetBundlePatterns": [ "assets/*" ],
    "version": "1.0.6",
    "orientation": "default",
    "githubUrl": "https://github.com/jgorman/darkest-night-clock-react-js",
    "primaryColor": "#000000",
    "icon": "./assets/clock-1024x1024.png",
    "splash": {
      "image": "./assets/clock-1024x1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "io.johngorman.darkest-night-clock",
      "buildNumber": "1.0.6",
      "icon": "./assets/clock-ios-1024x1024.png",
      "loadJSInBackgroundExperimental": true,
      "supportsTablet": true
    },
    "android": {
      "package": "io.johngorman.darkest_night_clock",
      "versionCode": 6
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
