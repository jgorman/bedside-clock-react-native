# npm install -g svgexport

splash=192:192
button=100:100

svgexport clock.svg ../assets/clock.png $splash

svgexport colors.svg colors.png $button
svgexport minus-circle.svg minus-circle.png $button
svgexport plus-circle.svg plus-circle.png $button
svgexport seconds.svg seconds.png $button
svgexport show-date.svg show-date.png $button
